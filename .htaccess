ErrorDocument 400 https://pggroup.tech/error/403/
ErrorDocument 401 https://pggroup.tech/error/403/
ErrorDocument 403 https://pggroup.tech/error/403/
ErrorDocument 404 https://pggroup.tech/error/403/
ErrorDocument 503 https://pggroup.tech/error/403/
php_value display_errors On
php_value mbstring.http_input auto
php_value date.timezone America/New_York
##################################################
#
# DO NOT EDIT THIS FILE DIRECTLY
#
# Create a new .htaccess file in your htdocs
# directory (or example.com/htdocs/ directory)
# to add your own rules or override these rules.
#
##################################################
DirectoryIndex index.php index.html index.htm index2.html
#remove php file extension-e.g. https://example.com/file.php will become https://example.com/file 
RewriteEngine on
RewriteCond %{REQUEST_FILENAME} !-d
RewriteCond %{REQUEST_FILENAME}\.php -f
RewriteRule ^(.*)$ $1.php [NC,L] 
#remove html file extension-e.g. https://example.com/file.html will become https://example.com/file
RewriteEngine on 
RewriteCond %{REQUEST_FILENAME} !-d
RewriteCond %{REQUEST_FILENAME}\.html -f
RewriteRule ^(.*)$ $1.html [NC,L]
